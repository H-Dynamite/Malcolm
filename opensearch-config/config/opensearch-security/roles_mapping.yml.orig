---
_meta:
  type: "rolesmapping"
  config_version: 2

all_access:
  reserved: true
  backend_roles:
  - "$ROLE_ADMIN"

dashboards_read_access:
  reserved: true
  backend_roles:
  - "$ROLE_READ_ACCESS"
  - "$ROLE_DASHBOARDS_READ_ACCESS"

dashboards_read_write_access:
  reserved: true
  backend_roles:
  - "$ROLE_DASHBOARDS_READ_WRITE_ACCESS"
